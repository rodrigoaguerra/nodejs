{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": ["import"],
    "settings": {
        "import/resolver": {
        "node": {
            "paths": ["src"]
        },
        "babel-module": {}
        }
    },
    "rules": {
        "no-underscore-dangle": "off",
        "no-use-before-define": "off",
        "no-console": "off",
        "import/prefer-default-export": "off",
        "consistent-return": "off",
        "no-plusplus": "off",
        "no-param-reassign": "off"
    }
}